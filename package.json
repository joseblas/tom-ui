{
  "name": "tom-ui",
  "version": "0.0.1",
  "description": "BDEC Entity extraction UI",
  "main": "src/index.js",
  "scripts": {
    "build": "webpack",
    "lint": "eslint --ext .js --ext .jsx ./",
    "lint:fix": "eslint --fix .js --ext .jsx ./",
    "serve": "NODE_ENV=development webpack-dev-server --host 0.0.0.0",
    "test": "npm run test:file -- 'src/**/*_specs.@(js|jsx)'",
    "test:file": "NODE_ENV=test mocha --compilers js:babel-core/register,css:src/utils/css_test_compiler --require ./src/utils/test_helper.js",
    "test:coverage": "node -r babel-core/register node_modules/.bin/isparta cover --include '**/*.js' --include '**/*.jsx' --report text --report lcov -x '*_specs.@(js|jsx)' _mocha -- --compilers js:babel-core/register,css:src/utils/css_test_compiler --recursive --require ./src/utils/test_helper.js 'src/**/*_specs.*'",
    "test:watch": "npm run test -- --watch --watch-extensions jsx"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:joseblas/tom-ui.git"
  },
  "keywords": [
    ""
  ],
  "author": "Dan Voyce <daniel.voyce@digital.hmrc.gov.uk>",
  "license": "UNLICENSED",
  "homepage": "https://github.com/joseblas/tom-ui",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.3.15",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-es2015-classes": "^6.18.0",
    "babel-plugin-transform-proto-to-assign": "^6.9.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-2": "^6.13.0",
    "chai": "^3.4.1",
    "chai-as-promised": "^6.0.0",
    "chai-immutable": "^1.5.3",
    "chance": "^1.0.3",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.23.0",
    "enzyme": "^2.3.0",
    "eslint": "^2.11.1",
    "eslint-plugin-react": "^5.1.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "handlebars-loader": "^1.4.0",
